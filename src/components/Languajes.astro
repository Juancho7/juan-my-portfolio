---
const languajes = [
  { languajeName: 'Spanish', level: 'Native Tanguage' },
  { languajeName: 'English', level: 'B1' },
];
---

<section>
  <h2 class='text-xl font-semibold'>Languajes</h2>
  <ul class='mt-4 flex flex-col gap-y-3'>
    {
      languajes.map((languaje) => (
        <li class='text-lg'>
          <strong>{languaje.languajeName}</strong>: {languaje.level}
        </li>
      ))
    }
  </ul>
</section>
